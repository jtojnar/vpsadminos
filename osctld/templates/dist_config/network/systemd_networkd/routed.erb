# This file is generated by osctld from vpsAdminOS. Do not edit this file, all
# changes will be lost. To stop osctld from manipulating this file, run
#
#   chmod u-w /etc/systemd/network/<%= netif.name %>.network

[Match]
Name=<%= netif.name %>

[Network]
DHCP=false

<% netif.active_ip_versions.each do |v| -%>
[Route]
Destination=<%= netif.default_via(v) %>
Scope=link

<%   netif.ips(v).each do |addr| -%>
[Address]
Address=<%= addr.to_string %>
<%   end -%>

[Route]
Gateway=<%= netif.default_via(v) %>
<% end -%>
